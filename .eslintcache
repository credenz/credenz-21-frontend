   })\r\n        .catch((err) => {});\r\n    } catch (err) {\r\n      console.error(err);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <BoxContainer className=\"p-4\">\r\n      <FormContainer>\r\n      <span hidden={!emailError} className=\"error-text\">\r\n          Please enter a valid E-Mail id.\r\n        </span>\r\n        <Input\r\n          disabled={emailSent}\r\n          type=\"email\"\r\n          placeholder=\"Email\"\r\n          value={email}\r\n          onChange={(e) => setEmail(e.target.value)}\r\n        />\r\n        <Marginer direction=\"vertical\" margin={20} />\r\n        <Input\r\n          className=\"mb-2\"\r\n          type=\"string\"\r\n          placeholder=\"Verfication Token\"\r\n          value={token}\r\n          onChange={(e) => setToken(e.target.value)}\r\n        />\r\n        <Input\r\n          type=\"password\"\r\n          placeholder=\"New Password\"\r\n          value={password}\r\n          onChange={(e) => setPassword(e.target.value)}\r\n        />\r\n        <span hidden={!passwordError} className=\"error-text\">\r\n          The password must contain atleast 1 aplhabet, 1 number and 1 special\r\n          character\r\n        </span>\r\n      </FormContainer>\r\n      <Marginer direction=\"vertical\" margin={70} />\r\n\r\n      <button\r\n        className=\"btn btn--secondary mb-1\"\r\n        type=\"submit\"\r\n        onClick={sendVerificationToken}\r\n      >\r\n        <span class=\"btn__content\">Send Verification Mail</span>\r\n        <span class=\"btn__glitch\"></span>\r\n      </button>\r\n      <button\r\n        className=\"btn btn--secondary\"\r\n        type=\"submit\"\r\n        onClick={handleForgetPassword}\r\n      >\r\n        <span class=\"btn__content\">Submit</span>\r\n        <span class=\"btn__glitch\"></span>\r\n      </button>\r\n    </BoxContainer>\r\n  );\r\n}\r\n","D:\\PICT\\PISB\\CREDENZ-21\\credenz-21-frontend\\src\\Components\\loginregister\\signupForm.jsx",[],"D:\\PICT\\PISB\\CREDENZ-21\\cre